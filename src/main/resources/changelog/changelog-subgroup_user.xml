<?xml version="1.1" encoding="UTF-8" standalone="no"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
            http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.1.xsd">

    <!-- SUBGROUP~USER -->
    <changeSet id="4-1" author="dstepanek">
        <createTable tableName="subgroup_user">
            <column name="subgroup_id" type="INT">
                <constraints nullable="false" foreignKeyName="subgroup_fk" references="subgroup(id)"/>
            </column>
            <column name="user_id" type="INT">
                <constraints nullable="false" foreignKeyName="user_fk" references="user(id)"/>
            </column>
        </createTable>
    </changeSet>

</databaseChangeLog>